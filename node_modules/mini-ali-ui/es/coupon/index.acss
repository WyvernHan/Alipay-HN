.am-coupon{position:relative;padding-top:24%;margin:6px 10px;background:0 0}.am-coupon-container{position:absolute;top:0;left:0;background:0 0;border-radius:8px;border-radius:var(--am-coupon-border-radius,8px);width:100%;height:100%;display:flex;align-items:stretch}.am-coupon-thumb{width:18%;background:#fff;background:var(--am-coupon-background-color,#fff);border-top-left-radius:8px;border-top-left-radius:var(--am-coupon-border-radius,8px);border-bottom-left-radius:8px;border-bottom-left-radius:var(--am-coupon-border-radius,8px);z-index:1;display:flex;align-items:center;justify-content:space-around}.am-coupon-thumb-image{width:42px;height:42px;border-radius:50vh;border-radius:var(--am-coupon-thumb-border-radius,50vh);background:grey;margin-left:6px}.am-coupon-divider{width:12.5px;background:0 0;display:flex;flex-direction:column}.am-coupon-divider-upper-hole{background:0 0;height:12.5px;margin-top:-8px;border-radius:50vh;box-shadow:0 17px 0 10px #fff;box-shadow:0 17px 0 10px var(--am-coupon-background-color,#fff)}.am-coupon-divider-middle-blank{background:#fff;background:var(--am-coupon-background-color,#fff);flex-grow:1;z-index:1;display:flex;justify-content:space-around}.am-coupon-divider-dashed-line{width:0}.am-coupon-divider-lower-hole{background:0 0;height:12.5px;margin-bottom:-8px;border-radius:50vh;box-shadow:0 -17px 0 10px #fff;box-shadow:0 -17px 0 10px var(--am-coupon-background-color,#fff)}.am-coupon-content{flex:1;background:#fff;border-top-right-radius:8px;border-bottom-right-radius:8px;display:flex;padding:12px 12px;padding:var(--am-coupon-content-spacing,12px) var(--am-coupon-content-spacing,12px)}.am-coupon-content-text{flex-grow:1;justify-content:space-around;align-self:center;margin-left:-6px}.am-coupon-content-text-title{font-size:18px;font-size:var(--am-coupon-title-size,18px);color:#333;color:var(--am-coupon-title-color,#333);font-family:PingFangSC-Medium}.am-coupon-content-text-subtitle{margin-top:4px;font-size:13px;font-size:var(--am-coupon-subtitle-size,13px);color:#999;color:var(--am-coupon-subtitle-color,#999);font-family:PingFangSC-Medium}.am-coupon-content-action{display:flex;align-items:center}.am-coupon-content-action-slot{display:flex}